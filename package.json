    "reselect": "^3.0.1",
    "semantic-ui-css": "^2.3.1",
    "semantic-ui-react": "^0.78.3",
    "source-map-support": "^0.5.4",
    "ws": "^5.1.1"
  },
  "devEngines": {
    "node": ">=8.x",
    "npm": ">=5.x",
    "yarn": ">=1.x"
  },
  "resolutions": {
    "electron-rebuild/node-abi": "2.3.0"
  }
}
